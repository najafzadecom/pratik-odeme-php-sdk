{
    "name": "najafzadecom/pratik-odeme-php-sdk",
    "description": "Pratik Ödeme Kurumsal Cüzdan API üçün PHP SDK",
    "type": "library",
    "keywords": ["pratik", "ödeme", "payment", "api", "sdk", "kurumsal", "cüzdan", "wallet"],
    "homepage": "https://github.com/najafzadecom/pratik-odeme-php-sdk",
    "support": {
        "issues": "https://github.com/najafzadecom/pratik-odeme-php-sdk/issues",
        "source": "https://github.com/najafzadecom/pratik-odeme-php-sdk"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kamran Najafzade",
            "email": "nadjafzadeh@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PratikOdeme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PratikOdeme\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse src --level=8",
        "phpcs": "phpcs src --standard=PSR12",
        "phpcbf": "phpcbf src --standard=PSR12"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}